{
  "name": "flowforge-frontend",
  "version": "0.5.0",
  "description": "Frontend for FlowForge - Visual GitHub Actions Workflow Builder",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "lint": "eslint src --ext js,jsx,ts,tsx",
    "lint:fix": "eslint src --ext js,jsx,ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md}\"",
    "clean": "rm -rf dist node_modules/.vite",
    "docker:build": "docker build -f Dockerfile.prod -t flowforge-frontend:latest .",
    "docker:run": "docker run -p 8080:8080 flowforge-frontend:latest"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "axios": "^1.12.0",
    "js-yaml": "^4.1.1",
    "lucide-react": "^0.303.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^7.9.6",
    "reactflow": "^11.10.1",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "terser": "^5.43.1",
    "typescript": "^5.3.3",
    "vite": "^6.4.1",
    "vitest": "^3.2.4"
  }
}
